# Redis 인증 비밀번호 예시
REDIS_PASSWORD=your_strong_password_here

# Gemini API Key (Google AI Studio에서 발급)
GOOGLE_API_KEY=your_google_api_key_here

# Gemini 모델 설정 (기본값: gemini-1.5-flash)
GEMINI_MODEL=gemini-1.5-flash

# 자동 저장 설정 (1이면 .taskmaster/docs/research/에 저장)
GEMINI_SAVE=1

# 기타 환경변수들
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/tickettock
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
